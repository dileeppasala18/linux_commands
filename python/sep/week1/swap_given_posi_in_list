# write a program to swap the elements at the given positions

def swap_elements(lst, a, b):
    lst[a], lst[b] = lst[b], lst[a]
    return lst

my_list = [10, 20, 30, 40, 50]
a = int(input("Enter the first position to swap: ")) - 1
b = int(input("Enter the second position to swap: ")) - 1

swapped_list = swap_elements(my_list,a, b)

print(f"List after swapping: {swapped_list}")
